algoritmo "Verificar Múltiplos ex12"
var
   a, b: inteiro
inicio
   escreva("Digite o primeiro número: ")
   leia(a)
   escreva("Digite o segundo número: ")
   leia(b)

   se (b % a = 0) entao
      escreval(b, " é múltiplo de ", a)
   senao
      escreval(b, " não é múltiplo de ", a)
   fimse
fimalgoritmo
