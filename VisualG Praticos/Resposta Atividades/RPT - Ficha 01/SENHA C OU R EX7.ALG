algoritmo "senha C ou R ex7"
var

   senha, tentativa : inteiro

inicio

   escreva("Crie a senha")
   leia (senha )
   limpatela
   escreva("Coloque a senha:")
   leia (tentativa)
   enquanto tentativa <> senha faca
      escreva("Senha incorreta. Digite Novamente...")
      leia (tentativa)
   fimenquanto
   escreva("Senha crr")





fimalgoritmo