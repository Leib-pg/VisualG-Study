algoritmo "semnome"
var

   media, n1, n2:real


inicio

   escreva("Digite a primeira nota do aluno:")
    leia(n1)
   escreva("Digite a primeira nota do aluno:")
    leia (n2)

LIMPATELA

     n1 <- n1 *3.5
     n2 <- n2 *6,5
     media <- (n1+n2)/10
   escreval("A media poderada foi.:",media)




fimalgoritmo