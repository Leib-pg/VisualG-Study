algoritmo "semnome"
var

   n1, n2: inteiro


inicio

   escreva("escreva um numero:")
   leia(n1)

   escreva("escreva outro numero:")
   leia(n2)

   escreval("Soma=", n1+n2)
   escreval("subtração=", n1-n2)
   escreval("multiplicação=", n1*n2)
   escreval("divisão=", n1/n2)

   se (n1 <> 0) ou (n2 <> 0) entao
      escreva("Nao é possivel fazer essa ")

   fimse



fimalgoritmo