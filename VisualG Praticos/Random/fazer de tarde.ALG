algoritmo "semnome"
var

   a, b, c : inteiro


inicio

   escreva("Digite um numero para a:")
   leia(a)
   escreva("Digite um numero para b:")
   leia(b)
   escreva("Digite um numero para c:")
   leia(c)
   se ( a < b ) ou ( a < c ) entao
      escreval(a)
      escreval(b)
      escreval(c)
   senao
      escreval(a)
      escreval(c)
      escreval(b)



   fimse


fimalgoritmo